programa {
  funcao inicio() {
    // 5. Escreva um algoritmo para imprimir os 10 
    // primeiros números inteiros maiores que 100.
    inteiro x = 100
    enquanto(x <= 110) {
      escreva(x, "\n")
      x++
    }

    escreva("--------------------\n")

    para (inteiro cont = 100; cont <= 110; cont++) {
      escreva(cont, "\n")
    }
  }
}
